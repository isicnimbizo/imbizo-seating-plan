  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Seating Plan",
      "type": "python",
      "request": "launch",
      "module": "beachbums",
      "justMyCode": true,
      "args": ["-vv", "-o", ""],
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      }
    },
    {
      "name": "Python: Group Plan (Programming)",
      "type": "python",
      "request": "launch",
      "module": "beachbums",
      "justMyCode": true,
      "args": ["--background", "Programming", "-vv", "-o", ""],
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      }
    },
    {
      "name": "Python: Report",
      "type": "python",
      "request": "launch",
      "module": "beachbums",
      "justMyCode": true,
      "args": ["--report", "-vv"],
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      }
    }
  ]
}
